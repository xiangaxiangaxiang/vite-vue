{
  "name": "vite-vue",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ts-check": "vue-tsc --noEmit",
    "build": "npm run ts-check && cross-env NODE_ENV=production vite build",
    "serve": "cross-env NODE_ENV=development vite",
    "preview": "vite preview",
    "eslint": "npx eslint ./src --ext .vue,.js,.ts",
    "eslint:fix": "npx eslint ./src --ext .vue,.js,.ts --fix",
    "stylelint": "npx stylelint \"src/**/*.vue\" \"src/**/*.less\"",
    "stylelint:fix": "npx stylelint --fix \"src/**/*.vue\" \"src/**/*.less\"",
    "pre-commit": "npm run ts-check && npm run test && lint-staged",
    "prepare": "husky install",
    "test": "jest -o"
  },
  "devDependencies": {
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/vue3-jest": "^29.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.1.1",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.9.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.1.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "element-plus": "^2.2.30",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
